package commands

import (
  "github.com/hofstadter-io/dsl-cli/spec"
)

Run : cli.Command & {

  usage: "run <command> [entrypoint]"
  aliases: [
    "r",
  ]
  short: "Run a command."
  long: "Run a command ."

  args: [
    {
      name: "command"
      type: "string"
      required: true
    },
    {
      name: "entrypoint"
      type: "string"
      default: "."
    },
  ]

}


