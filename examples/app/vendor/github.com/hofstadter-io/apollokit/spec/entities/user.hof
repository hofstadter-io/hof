package entities

User : {
  Personas: []string
  DefaultPersona: string

  Registration: {
    Username: {
      disabled: bool | *true
    }
  }

  Emails: {
    DefaultSender: EmailSender
    Register: Email
    ConfirmAccount: Email
    ForgotPassword: Email
    PasswordChanged: Email
  }
}

EmailSender : {
  Name: string
  Email: string
}

Email : {
  Sender: EmailSender
  Subject: string
  Html: common.FILEPATH_REGEX
  Text: common.FILEPATH_REGEX
}
